# API Keys - Copy this file to .env and fill in your actual keys
# IMPORTANT: Do NOT commit your .env file with actual keys to version control.
OPENAI_API_KEY=your_openai_key_here
ANTHROPIC_API_KEY=your_anthropic_key_here

# Optional: Custom model configurations
# Example: LLM_DEFAULT_MODEL=gpt-4o-mini
LLM_DEFAULT_MODEL=

# Security: Disable dangerous features like custom tool execution by default
# Set to 'false' or remove/comment out if you understand and accept the risks.
LLM_DISABLE_CUSTOM_TOOLS=true
